<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppCardView" parent="@style/Widget.MaterialComponents.CardView">
        <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay</item>
        <item name="strokeWidth">0dp</item>
        <item name="cardPreventCornerOverlap">false</item>
        <item name="android:backgroundTint">#BFE3C0</item>
    </style>

    <style name="ShapeAppearanceOverlay" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="android:elevation">0dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>

    <style name="AppTextTitle" parent="TextAppearance.Material3.TitleLarge">
        <item name="android:textSize">42sp</item>
    </style>


    <style name="AppTextLabel" parent="TextAppearance.Material3.LabelLarge">
    </style>

    <style name="AppTextLabel.Bold">
        <item name="android:textStyle">bold</item>
    </style>

        <style name="AppTextLabel.S16">
            <item name="android:textSize">16sp</item>
        </style>
</resources>